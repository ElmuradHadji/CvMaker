@model PrimaryData
@{
    ViewData["Title"] = "Create";
}



<body>
    <form method="post" class="forms-sample">
        <div class="form-group">
        <label asp-for="@Model.Name"></label>
        <input asp-for="@Model.Name">
            <span class="text text-danger" asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Surname"></label>
            <input asp-for="@Model.Surname">
            <span class="text text-danger" asp-validation-for="Surname"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Brithday"></label>
            <input asp-for="@Model.Brithday">
            <span class="text text-danger" asp-validation-for="Brithday"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Email"></label>
            <input asp-for="@Model.Email">
            <span class="text text-danger" asp-validation-for="Email"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.PhoneNumber"></label>
            <input asp-for="@Model.PhoneNumber">
            <span class="text text-danger" asp-validation-for="PhoneNumber"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Town"></label>
            <input asp-for="@Model.Town">
            <span class="text text-danger" asp-validation-for="Town"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Insta">Instagram Url</label>
            <input asp-for="@Model.Insta">
            <span class="text text-danger" asp-validation-for="Insta"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Fb">Facebook Url</label>
            <input asp-for="@Model.Fb">
            <span class="text text-danger" asp-validation-for="Fb"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Linkedin">Linkedin Url</label>
            <input asp-for="@Model.Linkedin">
            <span class="text text-danger" asp-validation-for="Linkedin"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Website"></label>
            <input asp-for="@Model.Website">
            <span class="text text-danger" asp-validation-for="Website"></span>
        </div>
        <button type="submit" class="btn btn-success mr-2">Next</button>
        <button class="btn btn-dark" asp-action="Index">Go back</button>
    </form>
</body>


